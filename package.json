{
  "name": "simple-axios",
  "description": "基于axios封装的请求库，可以对接口进行集中式管理，方便接口的使用和维护，配合mock功能在开发阶段便于程序的测试。",
  "version": "0.0.1",
  "main": "lib/index.js",
  "typings": "global.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yinjiazeng/simple-axios.git"
  },
  "keywords": [
    "request",
    "axios",
    "mock"
  ],
  "jest": {
    "transform": {
      "^.+\\.(j|t)s$": "babel-jest"
    }
  },
  "author": "yinjiazeng",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "qs": "^6.9.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-modules-commonjs": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/node": "^13.7.0",
    "@types/qs": "^6.9.1",
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.19.2",
    "babel-jest": "^25.1.0",
    "cross-env": "^7.0.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-typescript": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "tslib": "^1.10.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "lib",
    "dist",
    "global.d.ts"
  ],
  "scripts": {
    "build": "npm run build:lib && npm run build:dist",
    "build:lib": "npm run clean:lib && npm run build:cjs",
    "build:dist": "npm run clean:dist && npm run build:umd && npm run build:min",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src/*.ts --out-dir lib --extensions .ts --ignore src/types.ts",
    "build:umd": "cross-env BABEL_ENV=umd rollup --config",
    "build:min": "cross-env BABEL_ENV=umd NODE_ENV=production rollup --config",
    "clean:lib": "rimraf lib",
    "clean:dist": "rimraf dist",
    "format": "prettier --write **/*.ts",
    "lint": "npm run format && eslint --fix --ext .ts .",
    "test": "jest"
  },
  "bugs": {
    "url": "https://github.com/yinjiazeng/simple-axios/issues"
  },
  "homepage": "https://github.com/yinjiazeng/simple-axios#readme",
  "directories": {
    "example": "example",
    "lib": "lib",
    "test": "test"
  }
}
