{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": [
    "airbnb-base"
  ],
  "plugins": [
    "prettier",
    "@typescript-eslint"
  ],
  "rules": {
    "dot-notation": "off",
    "no-unused-vars": "off",
    "import/extensions": "off",
    "import/no-unresolved": "off"
  },
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module"
    }
  }
